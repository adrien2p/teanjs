{
   "compileOnSave": false,
   "compilerOptions": {
      "strict": true,
      "incremental": true,
      "downlevelIteration": true,
      "importHelpers": true,
      "module": "commonjs",
      "outDir": "./dist/out-tsc",
      "baseUrl": "./",
      "sourceMap": true,
      "declaration": false,
      "moduleResolution": "node",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "target": "es2015",
      "types": [
         "jest",
         "node"
      ],
      "typeRoots": [
         "node_modules/@types"
      ],
      "lib": [
         "es2017",
         "dom"
      ]
   },
   "angularCompilerOptions": {
      "strictInjectionParameters": true
   }
}
